//to read
#include <iostream>
#include <fstream>
#include <string>
using namespace std;

int main() {
    // Open the file
    ifstream file ("example.txt");

    // Check if the file is opened successfully
    if (!file.is_open()) {
        cerr << "Error opening the file." << endl;
        return 1;
    }

    // Read and display the contents of the file line by line
    string line;
    while (getline (file, line)) {
        cout << line << endl;
    }

    // Close the file
    file.close();
    return 0;
}
